{
  "hosting": {
    "target": "tagtokn",
    "public": "my-app/build",
    "ignore": ["firebase.json", "**/.*", "**/node_modules/"],
    "rewrites": [
      {
        "source": "/auth/instagram/callback",
        "function": "exchangeInstagramCode"
      },
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  }
}
